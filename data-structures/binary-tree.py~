class binaryTree(self):
  def __init__(self):
    pass

  def printMenu(self):
      menuList = [
                   'add node',
                   'print tree'
                 ]
      i = 1
      for x in menuList:
        print(str(i) + " " +x)

def main():
  bt = binaryTree()
  bt.printMenu()

if __name__ == '__main__':
  main()
